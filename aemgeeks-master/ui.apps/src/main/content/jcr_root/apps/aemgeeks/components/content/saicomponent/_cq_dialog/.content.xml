<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="sai"
    sling:resourceType="/libs/cq/gui/components/authoring/dialog"
    extraClientlibs="[sai.component]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="/libs/granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <name
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="First Name"
                        name="./name"/>
                    <number
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/numberfield"
                        fieldLabel="Phone Number"
                        name="./number"/>
                    <instaid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Insta ID"
                        name="./instaid"/>
                    <color
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/colorfield"
                        fieldLabel="Color"
                        name="./color"/>
                    <profession
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                        fieldLabel="Profession"
                        name="./profession">
                        <items jcr:primaryType="nt:unstructured">
                            <student
                                jcr:primaryType="nt:unstructured"
                                text="student"
                                value="student"/>
                            <employ
                                jcr:primaryType="nt:unstructured"
                                text="employ"
                                value="employ"/>
                            <business
                                jcr:primaryType="nt:unstructured"
                                text="business"
                                value="business"/>
                        </items>
                    </profession>
                    <checkbox
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Student"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/checkbox"
                        fieldLabel="Student"
                        name="./student"
                        value="Student"/>
                    <joinningdate
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/datepicker"
                        fieldLabel="Joining Date"
                        name="./date"/>
                    <multifield
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        fieldLabel="Multi">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./multifield">
                            <items jcr:primaryType="nt:unstructured">
                                <link
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Link"
                                    name="./link"/>
                                <radiogroup
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                    fieldLabel="What is the even number"
                                    name="./answer">
                                    <items jcr:primaryType="nt:unstructured">
                                        <telugu
                                            jcr:primaryType="nt:unstructured"
                                            text="59"
                                            value="59"/>
                                        <english
                                            jcr:primaryType="nt:unstructured"
                                            text="78"
                                            value="78"/>
                                        <hindi
                                            jcr:primaryType="nt:unstructured"
                                            text="43"
                                            value="43"/>
                                    </items>
                                </radiogroup>
                                <nested
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="[granite/ui/components/coral/foundation/form/multifield]">
                                    <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="[granite/ui/components/coral/foundation/container]">
                                        <items jcr:primaryType="nt:unstructured">
                                            <color
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/colorfield"
                                                fieldLabel="Colors"
                                                name="./colors"/>
                                        </items>
                                    </field>
                                </nested>
                            </items>
                        </field>
                    </multifield>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
