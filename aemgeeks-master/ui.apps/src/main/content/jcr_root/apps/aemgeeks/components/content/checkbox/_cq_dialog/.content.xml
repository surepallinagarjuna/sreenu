<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Checkbox"
    sling:resourceType="/libs/cq/gui/components/authoring/dialog"
    extraClientlibs="[sai.component,sai.component1]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="/libs/granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <number
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/numberfield"
                        fieldLabel="Phone Number"
                        name="./number"/>
                    <profession
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                        fieldLabel="Profession"
                        name="./profession">
                        <items jcr:primaryType="nt:unstructured">
                            <student
                                jcr:primaryType="nt:unstructured"
                                text="student"
                                value="student"/>
                            <employ
                                jcr:primaryType="nt:unstructured"
                                text="employ"
                                value="employ"/>
                            <business
                                jcr:primaryType="nt:unstructured"
                                text="business"
                                value="business"/>
                        </items>
                    </profession>
                    <checkbox
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/checkbox"
                        disabled="{Boolean}true"
                        name="./checkbox"
                        text="CheckBox"
                        value="{Boolean}true"/>
                    <checkbox1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/checkbox"
                        name="./checkbox1"
                        text="Checkbox1"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
